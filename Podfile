# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
inhibit_all_warnings!   #  关闭CocoaPods中的警告

target 'FRToolBox' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FRToolBox
  pod 'AFNetworking', :git => 'https://github.com/xfdev/AFNetworking.git'
  pod 'YYCategories', :git => 'https://github.com/xfdev/YYCategories.git'
  pod 'Masonry', :git => 'https://github.com/warpling/Masonry.git' # Fix SnapKit#566
  pod 'MBProgressHUD', '~> 1.2.0'       # 装机必备 - loading
  pod 'YYModel'                         # 装机必备 - 数据解析
  pod 'SDWebImage'                      # 装机必备 - 图片
  pod 'IQKeyboardManager'               # 装机必备 - 键盘
  pod 'STPopup'                         # 弹窗

end
